{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "message": "Project name"
    },
    "description": {
      "type": "string",
      "required": true,
      "message": "Project description",
      "default": "StayWoke Platform Project"
    },
    "author": {
      "type": "string",
      "required": true,
      "message": "Author",
      "default": "StayWoke <dev@staywoke.org>"
    },
    "license": {
      "type": "string",
      "required": true,
      "label": "License",
      "default": "MIT"
    }
  },
  "completeMessage": "{{#inPlace}}To get started:\n\n  npm install\n  npm run dev{{else}}To get started:\n\n  cd {{destDirName}}\n  npm install\n  npm run dev{{/inPlace}}"
}
